<h1>All your clients</h1>
<div class="row">
<% @owners.each do |owner| %>
    <div class="col mt-3 mb-3">
      <div class="card">
        <img src="" class="card-img-top" alt="...">
        <div class="card-body">
          <div class="d-flex justify-content-between mb-3">
            <div>
              <h5 class="card-title"><%= owner.first_name%> <%= owner.last_name %> </h5>
              <p class="card-text">Address: <%= owner.address %></p>
              <p class="card-text">Mobile number: <%= owner.mobile_number %> </p>
              <p class="card-text">Pet: <%= @dog.name %> </p>
            </div>
          </div>

          <div class="d-flex justify-content-between" >
            <div>
            <%= link_to 'Details', user_path(owner) %>
            </div>
            <div>
            <%= link_to 'Back', root_path %>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
