<h1> <%= @dog.name %>'s details</h1>
<div class="card">
<%= cl_image_tag @dog.photo.key, height: 300, width: 400, crop: :fill %>
  <p>Breed: <%= @dog.breed %></p>
  <p>Date of birth: <%= @dog.date_of_birth %></p>
  <p>Pre-existing condition:<%= @dog.condition %></p>
  <p>Medication: <%= @dog.medication %></p>
  <p>Weight: <%= @dog.weight %></p>


</div>


<% if current_user == @owner  %>
  <%= render "owner_show" %>
<% else %>
  <%= render "else_show" %>
<% end %>
